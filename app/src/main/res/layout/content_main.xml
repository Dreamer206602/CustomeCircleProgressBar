<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.mx.customecircleprogressbar.MainActivity"
    tools:showIn="@layout/activity_main">


    <com.mx.circleseebarlib.CircleSeekBar
        android:layout_margin="20dp"
        android:id="@+id/circleSeekBar"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_centerInParent="true"
        android:padding="8dp"
        app:wheel_cur_process="30"
        app:wheel_pointer_radius="10dp"
        app:wheel_reached_width="8dp"
        app:wheel_unreached_width="10dp"/>
</RelativeLayout>
